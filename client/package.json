{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --output-path=../dist/client",
    "build:prod": "npm run build -- --configuration=production",
    "build:stat": "npm run build:prod -- --stats-json",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint && stylelint src/**/*.scss && htmlhint src",
    "install:api": "npm i && cd api && npm i",
    "start:dev": "concurrently --raw \"ng s --host 0.0.0.0\"",
    "bundle-report": "webpack-bundle-analyzer dist/is/stats.json",
    "docs": "compodoc -p tsconfig.json -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build:prod"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@ngx-translate/core": "^11.0.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.23.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "compodoc": "0.0.41",
    "concurrently": "^4.1.0",
    "htmlhint": "^0.10.1",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "stylelint": "^9.9.0",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
